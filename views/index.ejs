<%- include('layouts/Header') %>

</div>

<!-- top Products -->
<div class="ads-grid py-sm-5 py-4">
    <div class="container py-xl-4 py-lg-2">
        <!-- tittle heading -->
        <h3 class="tittle-w3l text-center mb-lg-5 mb-sm-4 mb-3">
            <span>O</span>ur
            <span>N</span>ew
            <span>P</span>roducts</h3>
        <!-- //tittle heading -->
        <div class="row">
            <!-- product left -->
            <div class="agileinfo-ads-display col-lg-9">
                <div class="wrapper">
                    <!-- first section -->
                    <div class="product-sec1 px-sm-4 px-3 py-sm-5 py-3 mb-4">
                        <h3 class="heading-tittle text-center font-italic" style="margin-bottom: 0.5em;">New Brand Mobiles</h3>
                        <div class="row">
                            <% mobiles.forEach(function(m) { %>
                            <div class="col-md-4 product-men mt-">
                                <div class="men-pro-item simpleCart_shelfItem">
                                    
                                        <div class="men-thumb-item text-center">
                                            <img class="pimage" src="/product_images/<%= m.id %>/<%= m.image %>" alt="">
                                            <div class="men-cart-pro">
                                                <div class="inner-men-cart-pro">
                                                    <a target="_blank" href="/products/<%= m.category %>/<%= m.slug %>" class="link-product-add-cart">Quick View</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="item-info-product text-center border-top mt-">
                                            <h4 class="pt-1">
                                                <a target="_blank" href="/products/<%= m.category %>/<%= m.slug %>"><%= m.title %></a>
                                            </h4>
                                            <div class="info-product-price my-2">
                                                <% if(m.discount_price) { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(m.discount_price).toFixed(2) %></span>
                                                    <del class="item_price">$<%= parseFloat(m.price).toFixed(2) %></del>
                                                <% } else { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(m.price).toFixed(2) %></span>
                                                <% } %>
                                            </div>
                                            <div class="snipcart-details top_brand_home_details item_add single-item hvr-outline-out">
                                                <!-- <p class="button btn"><a href="/cart/add/<%= m.slug %>"> Add to cart</a></p> -->
                                                <a href="/cart/add/<%= m.slug %>"> <input type="button" value="Add to cart" class="button btn" /> </a>
                                            </div>
                                        </div>
                                </div>
                            </div>
                            <% }) %>
                        </div>
                    </div>
                    <!-- //first section -->
                    <!-- second section -->
                    
                    <div class="product-sec1 px-sm-4 px-3 py-sm-5  py-3 mb-4">
                        <h3 class="heading-tittle text-center font-italic" style="margin-bottom: 0.5em;">Men Clothes</h3>
                        <div class="row">
                            <% men.forEach(function(m) { %>
                            <div class="col-md-4 product-men mt-">  
                                <div class="men-pro-item simpleCart_shelfItem">
                                        <div class="men-thumb-item text-center">
                                            <img class="pimage" src="/product_images/<%= m.id %>/<%= m.image %>"" alt="">
                                            <div class="men-cart-pro">
                                                <div class="inner-men-cart-pro">
                                                    <a target="_blank" href="/products/<%= m.category %>/<%= m.slug %>" class="link-product-add-cart">Quick View</a>
                                                </div>
                                            </div>
                                        </div>
                                   
                                        <div class="item-info-product text-center border-top mt-4">
                                            <h4 class="pt-1">
                                                <a target="_blank" href="/products/<%= m.category %>/<%= m.slug %>"><%= m.title %></a>
                                            </h4>
                                            <div class="info-product-price my-2">
                                                <% if(m.discount_price) { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(m.discount_price).toFixed(2) %></span>
                                                    <del class="item_price">$<%= parseFloat(m.price).toFixed(2) %></del>
                                                <% } else { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(m.price).toFixed(2) %></span>
                                                <% } %>
                                            </div>

                                            <div class="snipcart-details top_brand_home_details item_add single-item hvr-outline-out">
                                                <div class="snipcart-details top_brand_home_details item_add single-item hvr-outline-out">
                                                    <a href="/cart/add/<%= m.slug %>"> <input type="button" value="Add to cart" class="button btn" /> </a>
                                                </div>
                                            </div>

                                        </div>   
                                </div>
                                
                            </div>
                            <% }) %> 
                        </div>
                    </div>
                    
                    <!-- //second section -->
                    <!-- third section -->
                    <div class="product-sec1 product-sec2 px-sm-5 px-3">
                        <div class="row">
                            <h3 class="col-md-4 effect-bg">Summer Carnival</h3>
                            <p class="w3l-nut-middle">Get Extra 10% Off</p>
                            <div class="col-md-8 bg-right-nut">
                                <img src="/images/image1.png" alt="">
                            </div>
                        </div>
                    </div>
                    <!-- //third section -->

                    <!-- fourth section -->
                    <div class="product-sec1 px-sm-4 px-3 py-sm-5  py-3 mt-4">
                        <h3 class="heading-tittle text-center font-italic" style="margin-bottom: 0.5em;">Large Appliances</h3>
                        <div class="row">
                            <% appliances.forEach(function(a) { %>
                                <div class="col-md-4 product-men mt-5">
                                    <div class="men-pro-item simpleCart_shelfItem">
                                        <div class="men-thumb-item text-center">
                                            <img class="pimage" src="/product_images/<%= a.id %>/<%= a.image %>" alt="">
                                            <div class="men-cart-pro">
                                                <div class="inner-men-cart-pro">
                                                    <a target="_blank" href="/products/<%= a.category %>/<%= a.slug %>" class="link-product-add-cart">Quick View</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="item-info-product text-center border-top mt-4">
                                            <h4 class="pt-1">
                                                <a target="_blank" href="/products/<%= a.category %>/<%= a.slug %>"> <%= a.title %></a>
                                            </h4>
                                            <div class="info-product-price my-2">
                                                <% if(a.discount_price) { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(a.discount_price).toFixed(2) %></span>
                                                    <del class="item_price">$<%= parseFloat(a.price).toFixed(2) %></del>
                                                <% } else { %>
                                                    <span class="mx-2 font-weight:bold" style="color:#d60404; font-size: 16px;">$<%= parseFloat(a.price).toFixed(2) %></span>
                                                <% } %>
                                            </div>

                                            <div class="snipcart-details top_brand_home_details item_add single-item hvr-outline-out">
                                                <div class="snipcart-details top_brand_home_details item_add single-item hvr-outline-out">
                                                    <a href="/cart/add/<%= a.slug %>"> <input type="button" value="Add to cart" class="button btn" /> </a>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            <% }) %>
                        </div>
                    </div>
                    <!-- //fourth section -->
                </div>
            </div>
            <!-- //product left -->

            <!-- product right -->
				<div class="col-lg-3 mt-lg-0 mt-4 p-lg-0">
					<div class="side-bar p-sm-4 p-3">
						<div class="search-hotel border-bottom py-2">
							<h3 class="agileits-sear-head mb-3">Search Here..</h3>
							<form action="/product/product-search" method="get">
								<input type="search" placeholder="Product name..." name="search" required="">
								<input type="submit" value=" ">
							</form>
						</div>
						<!-- price -->
						<div class="range border-bottom py-2">
							<h3 class="agileits-sear-head mb-3">Price</h3>
							<div class="w3l-range">
								<ul>	
									<li>
										<a href="/home/two-hundred-price" target="_blank">Under $200</a>
									</li>
									<li class="my-1">
										<a target="_blank" href="/home/btw-hundred-price1">$100 - $200</a>
									</li>
									<li>
										<a target="_blank" href="/home/btw-hundred-price2">$200 - $300</a>
									</li>
									<li class="my-1">
										<a target="_blank" href="/home/btw-hundred-price3">$300 - $400</a>
									</li>
									<li>
										<a target="_blank" href="/home/btw-hundred-price4">$400 - $500</a>
									</li>
									<li class="mt-1">
										<a target="_blank" href="/home/btw-hundred-price5">Over $500</a>
									</li>	
								</ul>
							</div>
						</div>
						<!-- //price -->
						<!-- discounts -->
						<div class="left-side border-bottom py-2">
							<h3 class="agileits-sear-head mb-3">Discount</h3>
							<ul>
								<li>
									<input type="checkbox" class="checked" id="my-checkbox2" onclick="isChecked2()">
									<span class="span">10% - 20%</span>
								</li>
								<li>
									<input type="checkbox" class="checked" id="my-checkbox3" onclick="isChecked3()">
									<span class="span">20% - 30%</span>
								</li>
								<li>
									<input type="checkbox" class="checked" id="my-checkbox4" onclick="isChecked4()">
									<span class="span">30% - 40%</span>
								</li>
								<li>
									<input type="checkbox" class="checked" id="my-checkbox5" onclick="isChecked5()">
									<span class="span">50% - 60%</span>
								</li>
								<li>
									<input type="checkbox" class="checked" id="my-checkbox6" onclick="isChecked6()">
									<span class="span">60% or More</span>
								</li>
							</ul>
						</div>
						<!-- //discounts -->
						<!-- reviews -->
						<div class="customer-rev border-bottom left-side">
							<h3 class="agileits-sear-head mb-3"></h3>
								
                            <img width="250" class="img-responsive" src="/images/girl2.jpg">
                        </div>
						<!-- //reviews -->
						<!-- electronics -->
						
						
						<!-- best seller -->
						<div class="f-grid py-2">
							<h3 class="agileits-sear-head mb-3">Best Seller</h3>
							<div class="box-scroll">
								<div class="scroll">
									<% women.forEach(function(w) { %>
										<div class="row my-4">
											<div class="col-lg-3 col-sm-2 col-3 left-mar">
												<img src="/product_images/<%= w.id %>/<%= w.image %>" alt="" class="img-fluid">
											</div>
											<div class="col-lg-9 col-sm-10 col-9 w3_mvd">
												<a href="/products/<%= w.category %>/<%= w.slug %>"><%= w.full_title %> </a>
												<a href="" class="price-mar mt-2">$<%= w.price %></a>
											</div>
										</div>
									<% }) %>

                                    <% miscellaneous.forEach(function(m) { %>
										<div class="row my-4">
											<div class="col-lg-3 col-sm-2 col-3 left-mar">
												<img src="/product_images/<%= m.id %>/<%= m.image %>" alt="" class="img-fluid">
											</div>
											<div class="col-lg-9 col-sm-10 col-9 w3_mvd">
												<a href="/products/<%= m.category %>/<%= m.slug %>"><%= m.full_title %> </a>
												<a href="" class="price-mar mt-2">$<%= m.price %></a>
											</div>
										</div>
									<% }) %>
								</div>
							</div>
						</div>
						<!-- //best seller -->
					</div>
					<!-- //product right -->
				</div>
                
        </div>
    </div>
</div>
<!-- //top products -->

<%- include('layouts/footer') %>